  "private": false,
  "scripts": {
    "start": "nf start -p 3000",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "postgres:dropdb": "dropdb --username=postgres homesinone-dev",
    "postgres:createdb": "createdb --username=postgres homesinone-dev",
    "sql:migratedb": "sequelize db:migrate",
    "resetdb": "yarn postgres:dropdb && yarn postgres:createdb && yarn sql:migratedb && yarn start"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "./jest.setup.js"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
